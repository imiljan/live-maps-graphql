{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-standard", "tslint-config-prettier"],
  "rules": {
    "ordered-imports": true,
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "no-console": false,
    "no-floating-promises": false,
    "member-access": false
  }
}
